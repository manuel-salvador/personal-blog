{
  "extends": "next/core-web-vitals",
  "rules": {
    "no-unused-vars": "warn",
    "import/order": [
      "warn",
      {
        "groups": ["builtin", "object", "external", "internal", "parent", "sibling", "index"],
        "pathGroups": [
          {
            "pattern": "~/**",
            "group": "external",
            "position": "after"
          }
        ],
        "newlines-between": "always"
      }
    ]
  }
}
